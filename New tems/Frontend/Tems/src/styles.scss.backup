/* === Dark Mode Base Styles === */
:root {
  --bg-primary: #ffffff;
  --bg-secondary: #f5f5f7;
  --bg-tertiary: #e5e5e7;
  --bg-elevated: #ffffff;
  --text-primary: #1d1d1f;
  --text-secondary: #6e6e73;
  --text-tertiary: #86868b;
  --border-primary: #d2d2d7;
  --border-secondary: #e5e5e7;
  --border-subtle: #f0f0f2;
  --accent-color: #007aff;
  color-scheme: light;
}

html.dark {
  --bg-primary: #1c1c1e;
  --bg-secondary: #2c2c2e;
  --bg-tertiary: #3a3a3c;
  --bg-elevated: #2c2c2e;
  --text-primary: #f5f5f7;
  --text-secondary: #a1a1a6;
  --text-tertiary: #86868b;
  --border-primary: #38383a;
  --border-secondary: #48484a;
  --border-subtle: #2f2f31;
  --accent-color: #0a84ff;
  color-scheme: dark;
}

/* === Semantic Dark Mode Utility Classes === */
.dm-bg-page { background-color: var(--bg-primary); }
.dm-bg-card { background-color: var(--bg-secondary); }
.dm-bg-elevated { background-color: var(--bg-elevated); }
.dm-bg-subtle { background-color: var(--bg-tertiary); }
.dm-text-primary { color: var(--text-primary); }
.dm-text-secondary { color: var(--text-secondary); }
.dm-text-tertiary { color: var(--text-tertiary); }
.dm-border { border-color: var(--border-primary); }
.dm-border-subtle { border-color: var(--border-subtle); }
.dm-divide > * + * { border-color: var(--border-primary); }

html {
  height: 100%;
}

body {
  min-height: 100%;
  margin: 0;
  font-family: Roboto, "Helvetica Neue", sans-serif;
  background-color: var(--bg-secondary);
  color: var(--text-primary);
  transition: background-color 0.2s ease, color 0.2s ease;
}

html.dark body {
  background-color: #1c1c1e;
}

formly-field-mat-radio {
  .mat-radio-group {
    display: inline-flex;
    flex-direction: column;
  }

  .mat-radio-button {
    margin: 5px;
  }
}

.success-snackbar {
  background: #5cdb95;
}

.error-snackbar {
  background: #c7493a;
}

.default-snackbar {
  background: white;
  color: black;

  .dark & {
    background: #3a3a3c;
    color: #f5f5f7;
  }
}

.mat-simple-snackbar .mat-button-wrapper {
  color: white;
}

.default-snackbar .mat-button-wrapper {
  color: black;

  .dark & {
    color: #f5f5f7;
  }
}

.fade-in {
  -webkit-animation: fade-in 1.2s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
  animation: fade-in 1.2s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
}


@-webkit-keyframes fade-in {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

@keyframes fade-in {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}



.load-wrapp {
  text-align: center;

  .load-wrapp:last-child {
    margin-right: 0;
  }

  .letter-holder {
    padding: 16px;
    margin: 0;
    text-align: center;
  }

  .letter {
    display: inline-block;
    font-size: 16px;
    font-weight: bold;
    color: #5548E0;
  }

  .load-6 .letter {
    animation-name: loadingF;
    animation-duration: 1.6s;
    animation-iteration-count: infinite;
    animation-direction: linear;
  }

  .l-1 {
    animation-delay: 0.48s;
  }

  .l-2 {
    animation-delay: 0.6s;
  }

  .l-3 {
    animation-delay: 0.72s;
  }

  .l-4 {
    animation-delay: 0.84s;
  }

  @keyframes loadingF {
    0% {
      opacity: 0;
    }

    100% {
      opacity: 1;
    }
  }
}

@media only screen and (max-width: 1000px) {
  .mat-slide-toggle {
    width: 100%;
    display: inline-block;
    flex: none;
  }
}

.center-svg {
  display: block;
  margin-left: auto;
  margin-right: auto;
  width: 50%;
  margin-top: 10px;
  min-width: 100px;
  max-width: 280px;
}

.medium-font-size {
  font-size: 20px;
}

.big-font-size {
  font-size: 32px;
}

.small-font-size {
  font-size: 10px;
}

.clickable {
  cursor: pointer;
}

@media screen and (max-width: 991px) {
  .page-body-wrapper {
    padding: 70px 7px 0px;
  }

  .mat-dialog-container{
    padding: 14px !important;
  }
}

.mat-dialog-container{
  min-width: 35vw;
}

.text-justify {
  text-align: justify;
}

// Stands for margin-auto
.m-a {
  margin: auto;
}

.h-0 {
  height: 0;
}


// Angular material components override
.mat-expansion-panel-header {
  height: auto !important;
}

.property-container {
  max-width: 800px;
  min-width: 270px;
}

.ngx-pagination{
  margin: 0 !important;
  padding: 0 !important;
}

.card {
  .limit {
    min-width: 280px !important;
  }
}

// Bootstrap component styles overrides
@media screen and (max-width: 991px) {
  .card-body {
    padding: 10px !important;
  }

  .page-body-wrapper {
    padding-left: 0 !important;
    padding-right: 0 !important;
  }
}

button {
  white-space: normal !important;
}

.mat-button {
  .mat-icon {
    margin: 0 !important;
  }
}

.mat-icon{
  margin: 0 !important;
  vertical-align: middle !important;
}

.cancel-margins {
  margin: 0 !important;
  padding: 0 !important;
  border: 0 !important;
}

.container, .container-fluid, .container-lg, .container-md, .container-sm, .container-xl {
  padding-right: 0 !important;
  padding-left: 0 !important;
  margin-right: 0 !important; 
  margin-left: 0 !important;
}

.profile-photo{
  border-radius: 50%;
}

.unoffensive-shadow{
  -webkit-box-shadow: 5px 5px 15px 5px rgba(0,0,0,0.16); 
  box-shadow: 5px 5px 15px 5px rgba(0,0,0,0.16);
}

// Intermitent light effect

@keyframes taadaa { 
  0% {
      opacity: 0.2;
  }
  
  30% {
      opacity: 1;
  }
      
  70% {
      opacity: 1
  }

  100% {
      opacity: 0.2;
  }
}

.intermitent-light{
  animation: taadaa 5s infinite;
}

/* ============================================
   DARK MODE - GLOBAL OVERRIDES
   ============================================ */

/* Card and Panel Styles */
html.dark {
  /* === CORE BACKGROUNDS === */
  .bg-white {
    background-color: var(--bg-secondary) !important;
  }
  
  .bg-gray-50, .bg-\[\#f5f5f7\], .bg-\[\#f5f5f5\] {
    background-color: var(--bg-primary) !important;
  }
  
  .bg-gray-100 {
    background-color: var(--bg-tertiary) !important;
  }
  
  /* === TEXT COLORS === */
  .text-gray-900, .text-gray-800 {
    color: var(--text-primary) !important;
  }
  
  .text-gray-700, .text-gray-600 {
    color: var(--text-secondary) !important;
  }
  
  .text-gray-500, .text-gray-400 {
    color: var(--text-tertiary) !important;
  }
  
  /* === BORDER COLORS === */
  .border-gray-200, .border-gray-100 {
    border-color: var(--border-primary) !important;
  }
  
  .border-gray-300 {
    border-color: var(--border-secondary) !important;
  }
  
  /* === FORM INPUTS === */
  input, textarea, select {
    background-color: var(--bg-secondary);
    color: var(--text-primary);
    border-color: var(--border-primary);
    
    &:focus {
      border-color: var(--accent-color);
      outline-color: var(--accent-color);
    }
    
    &::placeholder {
      color: var(--text-tertiary);
    }
  }
  
  /* === SNACKBARS === */
  .success-snackbar {
    background: #30d158 !important;
  }
  
  .error-snackbar {
    background: #ff453a !important;
  }
  
  .default-snackbar {
    background: var(--bg-secondary) !important;
    color: var(--text-primary) !important;
  }
  
  /* === AG GRID DARK MODE === */
  .ag-theme-quartz, .ag-theme-alpine, .ag-theme-material {
    --ag-background-color: var(--bg-secondary);
    --ag-header-background-color: var(--bg-tertiary);
    --ag-odd-row-background-color: var(--bg-primary);
    --ag-row-hover-color: rgba(255, 255, 255, 0.05);
    --ag-selected-row-background-color: rgba(10, 132, 255, 0.15);
    --ag-range-selection-background-color: rgba(10, 132, 255, 0.2);
    --ag-header-foreground-color: var(--text-primary);
    --ag-foreground-color: var(--text-primary);
    --ag-secondary-foreground-color: var(--text-secondary);
    --ag-border-color: var(--border-primary);
    --ag-row-border-color: var(--border-primary);
    background-color: var(--bg-secondary) !important;
    color: var(--text-primary) !important;
  }

  /* Deep AG-Grid surfaces */
  .ag-theme-quartz .ag-root,
  .ag-theme-alpine .ag-root,
  .ag-theme-material .ag-root,
  .ag-theme-quartz .ag-root-wrapper,
  .ag-theme-alpine .ag-root-wrapper,
  .ag-theme-material .ag-root-wrapper,
  .ag-theme-quartz .ag-root-wrapper-body,
  .ag-theme-alpine .ag-root-wrapper-body,
  .ag-theme-material .ag-root-wrapper-body,
  .ag-theme-quartz .ag-body,
  .ag-theme-alpine .ag-body,
  .ag-theme-material .ag-body,
  .ag-theme-quartz .ag-body-horizontal-scroll-viewport,
  .ag-theme-alpine .ag-body-horizontal-scroll-viewport,
  .ag-theme-material .ag-body-horizontal-scroll-viewport,
  .ag-theme-quartz .ag-body-viewport,
  .ag-theme-alpine .ag-body-viewport,
  .ag-theme-material .ag-body-viewport,
  .ag-theme-quartz .ag-center-cols-viewport,
  .ag-theme-alpine .ag-center-cols-viewport,
  .ag-theme-material .ag-center-cols-viewport,
  .ag-theme-quartz .ag-center-cols-container,
  .ag-theme-alpine .ag-center-cols-container,
  .ag-theme-material .ag-center-cols-container,
  .ag-theme-quartz .ag-floating-bottom,
  .ag-theme-alpine .ag-floating-bottom,
  .ag-theme-material .ag-floating-bottom,
  .ag-theme-quartz .ag-floating-top,
  .ag-theme-alpine .ag-floating-top,
  .ag-theme-material .ag-floating-top,
  .ag-theme-quartz .ag-pinned-left-cols-container,
  .ag-theme-alpine .ag-pinned-left-cols-container,
  .ag-theme-material .ag-pinned-left-cols-container,
  .ag-theme-quartz .ag-pinned-right-cols-container,
  .ag-theme-alpine .ag-pinned-right-cols-container,
  .ag-theme-material .ag-pinned-right-cols-container {
    background-color: var(--bg-secondary) !important;
    color: var(--text-primary) !important;
  }

  /* Header wrappers */
  .ag-theme-quartz .ag-header,
  .ag-theme-alpine .ag-header,
  .ag-theme-material .ag-header,
  .ag-theme-quartz .ag-header-viewport,
  .ag-theme-alpine .ag-header-viewport,
  .ag-theme-material .ag-header-viewport,
  .ag-theme-quartz .ag-header-container,
  .ag-theme-alpine .ag-header-container,
  .ag-theme-material .ag-header-container,
  .ag-theme-quartz .ag-header-row,
  .ag-theme-alpine .ag-header-row,
